{
  "name": "rlang",
  "displayName": "RLang",
  "description": "robot language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "typescript": "^5.8.3"
  },
  "contributes": {
    "languages": [
      {
        "id": "rlang",
        "aliases": [
          "rlang",
          "rlang"
        ],
        "extensions": [
          ".mod"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rlang",
        "scopeName": "source.mod",
        "path": "./syntaxes/rlang.tmLanguage.json"
      }
    ]
  }
}